<openerp>
    <data noupdate="1">
        <record id="seq_type_batch_leave_request" model="ir.sequence.type">
            <field name="name">Batch Leave Request</field>
            <field name="code">hr.batch_leave_request</field>
        </record>
        <record id="seq_type_batch_allocation_request" model="ir.sequence.type">
            <field name="name">Batch Allocation Request</field>
            <field name="code">hr.batch_allocation_request</field>
        </record>

        <record id="sequence_batch_leave_request" model="ir.sequence">
            <field name="name">Batch Leave Request</field>
            <field name="code">hr.batch_leave_request</field>
            <field name="padding">5</field>
            <field name="prefix">B-LR/%(year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
        <record id="sequence_batch_allocation_request" model="ir.sequence">
            <field name="name">Batch Allocation Request</field>
            <field name="code">hr.batch_allocation_request</field>
            <field name="padding">5</field>
            <field name="prefix">B-ALR/%(year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
    </data>
</openerp>
